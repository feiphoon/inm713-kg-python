<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#"
     xml:base="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial">
        <rdfs:comment>A tutorial ontology for the Pizza domain</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasBase -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasBase">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#isBaseOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#PizzaBase"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasIngredient -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasIngredient">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasSpiciness -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasSpiciness">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#PizzaTopping"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#Spiciness"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasTopping -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasTopping">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasIngredient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#isToppingOf"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#Pizza"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#PizzaTopping"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#isBaseOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#isBaseOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#isIngredientOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#isIngredientOf"/>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#isMilderThan -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#isMilderThan">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#isSpicierThan"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#Spiciness"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#Spiciness"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#isSpicierThan -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#isSpicierThan">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#Spiciness"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#Spiciness"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#isToppingOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#isToppingOf">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#isIngredientOf"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasCaloricContent -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasCaloricContent">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#Pizza"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#AmericanaHotPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#AmericanaHotPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#JalapenoPepperTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#AmericanaPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#AmericanaPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#PepperoniTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A pizza that only has Mozzarella, Tomato, and Pepperoni toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#AnchovyTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#AnchovyTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#SeafoodTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#CaperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#CaperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#CheeseTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#CheeseTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#PizzaTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#CheesyPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#CheesyPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasTopping"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#CheeseTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#DeepPanBase -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#DeepPanBase">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#PizzaBase"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#ThinAndCrispyBase"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#GreenPepperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#GreenPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#PepperTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#HamTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#HamTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#MeatTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#HighCaloriePizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#HighCaloriePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasCaloricContent"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:minInclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#InterestingPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#InterestingPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasTopping"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">3</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#PizzaTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#JalapenoPepperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#JalapenoPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#PepperTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasSpiciness"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#Hot"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#LowCaloriePizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#LowCaloriePizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasCaloricContent"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
                                <owl:withRestrictions rdf:parseType="Collection">
                                    <rdf:Description>
                                        <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">400</xsd:maxExclusive>
                                    </rdf:Description>
                                </owl:withRestrictions>
                            </rdfs:Datatype>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#MargheritaPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#MargheritaPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#NamedPizza"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#MozzarellaTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasTopping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#TomatoTopping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasTopping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#MozzarellaTopping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#TomatoTopping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A pizza that only has Mozzarella and Tomato toppings</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#MeatTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#MeatTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#PizzaTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#MozzarellaTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#MozzarellaTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#CheeseTopping"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#ParmesanTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#MushroomTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#MushroomTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#NamedPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#NamedPizza">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#Pizza"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#OliveTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#OliveTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#ParmesanTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#ParmesanTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#CheeseTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#PepperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#PepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#VegetableTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#PepperoniTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#PepperoniTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#MeatTopping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#Pizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#Pizza">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#hasBase"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/fei/ontologies/2021/2/pizza-tutorial#PizzaBase"/>
            </owl:Restriction>
